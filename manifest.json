{
  "name": "Facebook Theme: Dark Facebook",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Give facebook a dark look with a black theme",
  "homepage_url": "http://www.daveeddy.com",
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "css": [
        "dark-facebook.css"
      ]
    }
  ]
}
